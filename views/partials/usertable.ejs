<table>
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Celular</th>
      <th scope="col">Senha</th>
    </tr>
  </thead>
  <tbody>
    <% let i; for ( i = 0; i < dados.length; i++) { %>
    <tr
      class="<%= dados[i].username == req.session.username ? 'destaque' : '' %>"
    >
      <th><%= dados[i].id %></th>
      <td><%= dados[i].username %></td>
      <td><%= dados[i].email %></td>
      <td><%= dados[i].celular %></td>
      <td><%= dados[i].password %></td>
    </tr>
    <% } %>
    <tfoot>
      <tr>
        <th colspan="5"> <%= `UsuÃ¡rios Registrados : ${dados.length}`%> </th>
      </tr>
    </tfoot>
  </tbody>
</table>
