<table>
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Celular</th>
    </tr>
  </thead>

  <tbody>
    <% for( let i = 0; i < 8; i++ ) { %>
    <tr>
      <td><%= i %></td>
      <td>Name</td>
      <td>Email</td>
      <td>Celular</td>
    </tr>
  </tbody>
  <% } %>
</table>

